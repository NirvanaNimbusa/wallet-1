require! {
    \./get-primary-coin.ls
}
themes =
    light:
        background: \#FFF
        input: \#FFF
        icon: \gray
        border: \#CCCCCC
        text: \black
    dark:
        background: \#213040
        input: \#213040
        icon: \#276A9F
        border: \#213040
        text: \#FFF
    dark_mojave:
        background: \#292A2F
        input: \#213040
        icon: \#276A9F
        border: \#213040
        text: \#FFF
    monochrome:
        background: \#000000
        input: \#213040
        icon: \#276A9F
        border: \#213040
        text: \#FFF
default-logo = "data:image/png;base64,"
module.exports = (store)->
    coin = get-primary-coin store
    branding =
        logo: coin.branding?logo ? default-logo
        title: coin.branding?title ? "WEB3 WALLET"
    links = coin.links ? []
    color = coin?color ? "#000000"
    app = themes[store.theme] ? themes.dark
    { app, branding, links, color }